{
  "connection": "default",
  "info": {
    "name": "import",
    "description":
      "Record an executed import and relate its products for possible later deletion"
  },
  "attributes": {
    "date": {
      "type": "date"
    },
    "url": {
      "type": "string"
    },
    "contentType": {
      "type": "string"
    },
    "fieldMapping": {
      "type": "json"
    },
    "importedItems": {
      "collection": "importeditem",
      "via": "import"
    },
    "ongoing": {
      "type": "boolean"
    },
    "progress": {
      "type": "float"
    }
  },
  "collectionName": "import_import"
}
