{
  "connection": "default",
  "info": {
    "name": "importconfig",
    "description":
      "Record an executed import and relate its products for possible later deletion"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "date": {
      "type": "date"
    },
    "source": {
      "type": "string"
    },
    "options": {
      "type": "json"
    },
    "contentType": {
      "type": "string"
    },
    "fieldMapping": {
      "type": "json"
    },
    "ongoing": {
      "type": "boolean"
    },
    "importeditems": {
      "collection": "importeditem",
      "via": "importconfig",
      "plugin": "import-content"
    }
  }
}
